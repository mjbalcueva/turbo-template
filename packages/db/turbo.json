{
	"$schema": "https://turborepo.com/schema.json",
	"extends": ["//"],
	"tasks": {
		"build": {
			"outputs": ["dist/**"]
		},
		"lint": {
			"outputs": []
		},
		"push": {
			"cache": false,
			"interactive": true
		},
		"studio": {
			"cache": false,
			"persistent": true
		},
		"generate": {
			"cache": false
		},
		"migrate": {
			"cache": false
		}
	}
}
